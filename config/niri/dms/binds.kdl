// ! CHANGES WILL BE OVERWRITTEN !
// ! PLACE YOUR CUSTOM CONFIGURATION ELSEWHERE !

binds {
    Mod+L hotkey-overlay-title="Lock Screen" { spawn "dms" "ipc" "call" "lock" "lock"; }
    Mod+X hotkey-overlay-title="Power Menu: Toggle" { spawn "dms" "ipc" "call" "powermenu" "toggle"; }
    Mod+space hotkey-overlay-title="App Launcher: Toggle" { spawn "dms" "ipc" "call" "spotlight" "toggle"; }
    Mod+Alt+Down hotkey-overlay-title="Move Workspace Down" { move-workspace-down; }
    Mod+Alt+Up hotkey-overlay-title="Move Workspace Up" { move-workspace-up; }
    Mod+G hotkey-overlay-title="Expand to Available Width" { expand-column-to-available-width; }
    Mod+Z hotkey-overlay-title="Toggle Overview" { toggle-overview; }
    Mod+Shift+E hotkey-overlay-title="Quit" { quit; }
    Mod+Shift+Slash { show-hotkey-overlay; }
    Mod+Return hotkey-overlay-title="Open Terminal: kitty" { spawn "kitty"; }
    Mod+W { close-window; }
    Mod+F { toggle-window-floating; }
    Mod+H hotkey-overlay-title="AI Chat" { spawn "kitty" "--class" "floating" "aichat"; }
    Print { screenshot; }
    Mod+P { screenshot; }
    Ctrl+Print { screenshot-screen; }
    Alt+Print { screenshot-window; }
    Mod+T { toggle-column-tabbed-display; }
    XF86AudioRaiseVolume allow-when-locked=true { spawn-sh "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+"; }
    XF86AudioLowerVolume allow-when-locked=true { spawn-sh "wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-"; }
    XF86AudioMute allow-when-locked=true { spawn-sh "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle"; }
    XF86AudioPlay allow-when-locked=true { spawn "playerctl" "play-pause"; }
    XF86AudioPrev allow-when-locked=true { spawn "playerctl" "previous"; }
    XF86AudioNext allow-when-locked=true { spawn "playerctl" "next"; }
    XF86Messenger { spawn "playerctl" "previous"; }
    XF86Go { spawn "playerctl" "play-pause"; }
    Cancel { spawn "playerctl" "next"; }
    XF86MonBrightnessUp allow-when-locked=true { spawn-sh "brightnessctl -d '*b*' s +5%"; }
    XF86MonBrightnessDown allow-when-locked=true { spawn-sh "brightnessctl -d '*b*' s 5%-"; }
    Mod+M { maximize-column; }
    Mod+Left { focus-column-left; }
    Mod+Right { focus-column-right; }
    Mod+Up { focus-window-up; }
    Mod+Down { focus-window-down; }
    Mod+N { focus-column-left; }
    Mod+O { focus-column-right; }
    Mod+I { focus-window-up; }
    Mod+E { focus-window-down; }
    Mod+Ctrl+Left { move-column-left; }
    Mod+Ctrl+Right { move-column-right; }
    Mod+Ctrl+Up { move-window-up; }
    Mod+Ctrl+Down { move-window-down; }
    Mod+Ctrl+N { move-column-left; }
    Mod+Ctrl+O { move-column-right; }
    Mod+Ctrl+I { move-window-up; }
    Mod+Ctrl+E { move-window-down; }
    Mod+Shift+Ctrl+Left { move-column-to-workspace-up; }
    Mod+Shift+Ctrl+Right { move-column-to-workspace-down; }
    Mod+Shift+Ctrl+N { move-column-to-workspace-up; }
    Mod+Shift+Ctrl+O { move-column-to-workspace-down; }
    Mod+1 { focus-workspace 1; }
    Mod+2 { focus-workspace 2; }
    Mod+3 { focus-workspace 3; }
    Mod+4 { focus-workspace 4; }
    Mod+5 { focus-workspace 5; }
    Mod+6 { focus-workspace 6; }
    Mod+7 { focus-workspace 7; }
    Mod+8 { focus-workspace 8; }
    Mod+9 { focus-workspace 9; }
    Mod+0 { focus-workspace 10; }
    Mod+Shift+1 { move-column-to-workspace 1; }
    Mod+Shift+2 { move-column-to-workspace 2; }
    Mod+Shift+3 { move-column-to-workspace 3; }
    Mod+Shift+4 { move-column-to-workspace 4; }
    Mod+Shift+5 { move-column-to-workspace 5; }
    Mod+Shift+6 { move-column-to-workspace 6; }
    Mod+Shift+7 { move-column-to-workspace 7; }
    Mod+Shift+8 { move-column-to-workspace 8; }
    Mod+Shift+9 { move-column-to-workspace 9; }
    Mod+Shift+0 { move-column-to-workspace 10; }
    Mod+WheelScrollDown cooldown-ms=150 { focus-workspace-down; }
    Mod+WheelScrollUp cooldown-ms=150 { focus-workspace-up; }
    Mod+Shift+Left { focus-workspace-up; }
    Mod+Shift+Right { focus-workspace-down; }
    Mod+Shift+N { focus-workspace-up; }
    Mod+Shift+O { focus-workspace-down; }
    Mod+Alt+Left { move-workspace-to-monitor-left; }
    Mod+Alt+Right { move-workspace-to-monitor-right; }
    Mod+Minus { set-column-width "-10%"; }
    Mod+Equal { set-column-width "+10%"; }
    Mod+Shift+Minus { set-window-height "-10%"; }
    Mod+Shift+Equal { set-window-height "+10%"; }
    Mod+R { switch-preset-column-width; }
    Mod+Shift+F { fullscreen-window; }
    Mod+C { center-column; }
    Mod+Comma { consume-window-into-column; }
    Mod+Period { expel-window-from-column; }
    Mod+BracketLeft { consume-or-expel-window-left; }
    Mod+BracketRight { consume-or-expel-window-right; }
    Mod+Shift+V { switch-focus-between-floating-and-tiling; }
    Mod+Escape allow-inhibiting=false { toggle-keyboard-shortcuts-inhibit; }
    Mod+Shift+Q { quit; }
    Mod+Ctrl+Shift+Alt+Delete { quit skip-confirmation=true; }
}
